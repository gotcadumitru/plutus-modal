  "scripts": {
    "start": "ENV=dev webpack serve --https --env port=3000",
    "start:prod": "ENV=prod webpack serve --https --env port=3000",
    "build:dev": "ENV=dev webpack --env mode=production",
    "build:dev:analyzer": "ENV=dev webpack --env mode=production analyzer=enabled",
    "build:prod": "ENV=prod webpack --env mode=production",
    "build:prod:analyzer": "ENV=prod webpack --env mode=production analyzer=enabled",
    "test:unit": "jest --config \"./config/jest/jest.config.ts\"",
    "test:ui": "npx loki test",
    "test:ui:ok": "npx loki approve",
    "test:ui:ci": "npx loki --requireReference --reactUri file:\"./storybook-static\"",
    "test:ui:report:ci": "npm run test:ui:json && npm run test:ui:html",
    "test:ui:report": "yarn test:ui:report:ci && open \".loki/report.html\"",
    "test:ui:json": "node scripts/generate-visual-json-report.ts",
    "test:ui:html": "reg-cli --from .loki/report.json --report .loki/report.html",
    "lint:ts": "eslint \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:ts:fix": "eslint --fix \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "lint:scss": "npx stylelint \"src/**/*.{scss,css}\"",
    "lint:scss:fix": "npx stylelint \"src/**/*.{scss,css}\" --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,md,json}\" --config \"./.prettierrc\"",
    "storybook": "storybook dev -p 6006 -c \"./config/storybook\"",
    "storybook:build": "storybook build -c \"./config/storybook\"",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "loki": {
    "diffingEngine": "pixelmatch",
    "configurations": {
      "chrome.iphonex": {
        "target": "chrome.docker",
        "width": 375,
        "height": 812,
        "mobile": true
      }
    },
    "pixelmatch": {
      "threshold": 0.1,
      "includeAA": false
    }
  },
